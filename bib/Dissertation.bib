@article{chen_statistical_2015,
	title = {Statistical analysis of {Q}-matrix based diagnostic classification models},
	volume = {110},
	issn = {0162-1459},
	url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC4539161/},
	doi = {10.1080/01621459.2014.934827},
	abstract = {Diagnostic classification models have recently gained prominence in
educational assessment, psychiatric evaluation, and many other disciplines.
Central to the model specification is the so-called Q-matrix
that provides a qualitative specification of the item-attribute relationship. In
this paper, we develop theories on the identifiability for the
Q-matrix under the DINA and the DINO models. We further
propose an estimation procedure for the Q-matrix through the
regularized maximum likelihood. The applicability of this procedure is not
limited to the DINA or the DINO model and it can be applied to essentially all
Q-matrix based diagnostic classification models. Simulation
studies are conducted to illustrate its performance. Furthermore, two case
studies are presented. The first case is a data set on fraction subtraction
(educational application) and the second case is a subsample of the National
Epidemiological Survey on Alcohol and Related Conditions concerning the social
anxiety disorder (psychiatric application).},
	number = {510},
	journal = {Journal of the American Statistical Association},
	author = {Chen, Yunxiao and Liu, Jingchen and Xu, Gongjun and Ying, Zhiliang},
	year = {2015},
	pmid = {26294801},
	pmcid = {PMC4539161},
	pages = {850--866},
	file = {PubMed Central Full Text PDF:/Users/jakethompson/Zotero/storage/GMC8CH2T/Chen et al. - 2015 - Statistical Analysis of Q-matrix Based Diagnostic .pdf:application/pdf}
}

